    "definitions": {
        "common.State": {
            "type": "object",
            "properties": {
                "data": {
                    "description": "失败时返回空"
                },
                "status_code": {
                    "description": "状态码，0-成功，其他值-失败",
                    "type": "integer"
                },
                "status_msg": {
                    "description": "返回状态描述",
                    "type": "string"
                }
            }
        },
        "common.Token": {
            "type": "object",
            "properties": {
                "expires_at": {
                    "type": "string"
                },
                "token": {
                    "type": "string"
                }
            }
        },
        "reply.AccountInfoReply": {
            "type": "object",
            "properties": {
                "accountID": {
                    "type": "integer"
                },
                "avatar": {
                    "type": "string"
                },
                "gender": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "signature": {
                    "type": "string"
                }
            }
        },
        "reply.CreateAccountReply": {
            "type": "object",
            "properties": {
                "accountID": {
                    "type": "integer"
                },
                "gender": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "token": {
                    "$ref": "#/definitions/common.Token"
                }
            }
        },
        "reply.LoginReply": {
            "type": "object",
            "properties": {
                "AccessToken": {
                    "type": "string"
                },
                "Payload": {
                    "$ref": "#/definitions/token.Payload"
                },
                "RefreshToken": {
                    "type": "string"
                },
                "UserID": {
                    "type": "integer"
                }
            }
        },
        "reply.TotalAccountsReply": {
            "type": "object",
            "properties": {
                "accountInfos": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/reply.AccountInfoReply"
                    }
                },
                "total": {
                    "type": "integer"
                }
            }
        },
        "request.CheckEmailExist": {
            "type": "object",
            "required": [
                "email"
            ],
            "properties": {
                "email": {
                    "description": "检查是否存在的邮箱",
                    "type": "string"
                }
            }
        },
        "request.CreateAccountReq": {
            "type": "object",
            "required": [
                "avatar",
                "gender",
                "name"
            ],
            "properties": {
                "ID": {
                    "type": "integer"
                },
                "avatar": {
                    "description": "头像图片",
                    "type": "string"
                },
                "gender": {
                    "description": "性别",
                    "type": "integer"
                },
                "name": {
                    "description": "昵称",
                    "type": "string"
                },
                "sigNature": {
                    "description": "个性签名",
                    "type": "string"
                }
            }
        },
        "request.Login": {
            "type": "object",
            "required": [
                "email",
                "loginType"
            ],
            "properties": {
                "email": {
                    "description": "邮箱",
                    "type": "string"
                },
                "email_code": {
                    "description": "之间发送的邮箱验证码",
                    "type": "string"
                },
                "loginType": {
                    "description": "1表示使用邮箱登录, 2表示使用密码登录",
                    "type": "integer"
                },
                "password": {
                    "description": "密码",
                    "type": "string"
                }
            }
        },
        "request.Register": {
            "type": "object",
            "required": [
                "email",
                "email_code",
                "mobile",
                "rePassword"
            ],
            "properties": {
                "email": {
                    "description": "注册邮箱",
                    "type": "string"
                },
                "email_code": {
                    "description": "邮箱验证码",
                    "type": "string"
                },
                "mobile": {
                    "description": "手机",
                    "type": "string"
                },
                "password": {
                    "description": "密码",
                    "type": "string",
                    "maxLength": 12,
                    "minLength": 3
                },
                "rePassword": {
                    "description": "第二次输出密码",
                    "type": "string"
                }
            }
        },
        "request.ReqModifyPassword": {
            "type": "object",
            "required": [
                "emailCode",
                "password"
            ],
            "properties": {
                "emailCode": {
                    "description": "身份认证的验证码",
                    "type": "string"
                },
                "password": {
                    "description": "修改成的密码",
                    "type": "string"
                }
            }
        },
        "request.SendEmail": {
            "type": "object",
            "required": [
                "email"
            ],
            "properties": {
                "email": {
                    "description": "发送邮箱的验证码",
                    "type": "string"
                }
            }
        },
        "token.Payload": {
            "type": "object",
            "properties": {
                "content": {
                    "description": "可以是用户或者是账户",
                    "type": "array",
                    "items": {
                        "type": "integer"
                    }
                },
                "expired-at": {
                    "type": "string"
                },
                "id": {
                    "description": "用于管理每个JWT",
                    "type": "string"
                },
                "issued-at": {
                    "description": "创建时间用于检验",
                    "type": "string"
                }
            }
        }
    },
    "securityDefinitions": {
        "BasicAuth": {
            "type": "basic"
        }
    }
